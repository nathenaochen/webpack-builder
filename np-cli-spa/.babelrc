{
  "presets": [
    ["@babel/preset-env", {"modules": false}], 
    "@babel/preset-react"
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", {
      "useESModules": true, 
      "corejs": {"version": 3, "proposals": true}
    }],
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from"
  ]
}
